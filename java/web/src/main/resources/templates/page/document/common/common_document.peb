				$("#path_nm").text(gaiaCommon.decodeSafeText("{{ message('item.doc.074') }}"));	// 이동할 경로

				$("#docMoveCopy").addClass('open');
			},

			//문서 복사 모달창 이동
			moveDocCopy() {
				this.docMoveCopyMode = "copy";
				const selectedDocData = this.grid.getCheckedRows();
				const docPath = page.document.getBreadCrumbString(page.document.data.breadCrumbList);


				if (selectedDocData.length <= 0) {
					gaiaCommon.customAlert("{{ message('msg.doc.059') }}"); // 복사할 문서를 선택해주세요.
