				});

			},

			//문서 이름 변경
			docRename() {
				// 입력값 수집 및 XSS 방지
				const sanitizedInput = (val) => val?.replace(/[<>]/g, "") || "";

				const newDocNm = $("#after_doc_nm").val();
				const isValidChecked = $('#docNmValid').val();

				if (!newDocNm) {
					gaiaCommon.customAlert("{{ message('msg.doc.081') }}"); // 변경할 문서 명을 입력해주세요.
					$("#after_doc_nm").focus();
					return false;
				}

				if (isValidChecked === 'N') {
					gaiaCommon.customAlert("{{ message('msg.doc.082') }}"); // 중복체크를 진행해주세요.
					$("#after_doc_nm").focus();
					return false;
				}

				let data = {
					docNm: sanitizedInput(newDocNm),
					docNo: this.data.selected.doc_no,
					docId: this.data.selected.doc_id
				}

				gaiaCommon.post("/api/document/name/update", data, function (result) {
					if (result.ok) {
						gaiaCommon.customAlert("{{ message('msg.doc.083') }}"); // 문서 명이 변경되었습니다.

						$('#updateDocName').removeClass('open');
						page.document.data.selected = {}; //선택된 데이터 초기화
						page.refresh(false, true);
					} else {
						gaiaCommon.customAlert("{{ message('msg.doc.084') }}"); // 문서 명 변경에 실패했습니다.
					}
				});
			},

			//문서 속성 정보 수정 - 모달창 이동
			moveUpdateProperty(rowKey) {
				const naviId = page.navigation.data.selected.naviId;
				const rowData = page.document.grid.getRow(rowKey);
				let renderHtml; // html 타입 속성 입력 요소

				$("#doc_property_u_tit").text(gaiaCommon.decodeSafeText("{{ message('item.doc.034') }}")); // 속성 정보 수정

				//선택한 문서 데이터 설정
				this.data.selected = rowData;

				const data = {
					docNo: rowData.doc_no,
					naviId: naviId,
					actionTy: "update"
				}

				// 문서 이름 기본값 설정
				$("#p_docNm").val(gaiaCommon.decodeSafeText(rowData.doc_nm));

				console.log("rowData", rowData);

				//선택된 트리 노드의 속성 데이터 가져와서 html 요소 그리기
				gaiaCommon.post("/api/document/add-item/html/create", data, function (result) {
					const dynamicFieldsContainer = document.querySelector("#updateProperty #dynamicFieldsContainer_update");
					dynamicFieldsContainer.querySelectorAll(".row.dynamic-row").forEach(row => row.remove());

					const propertyList = result.details.propertyList;

					console.log(rowData.propertyAttachment);
					let attrCdList = [];
					let makeAttachment = false; // 파일 업로드 수정 입력창 여부
					if (propertyList.length > 0) {
						propertyList.forEach(item => {
							const rowDiv = document.createElement("div");
							rowDiv.classList.add("row", "dynamic-row");

							const options = item.options ? JSON.parse(item.options.value) : []; // options가 있는 경우에만 파싱

							let inputHtml = "";

							if (item.attrbt_type === 'DAT') {
								inputHtml = `<input type="date" class="w-md date" id="${item.attrbt_cd}" name="${item.attrbt_cd}" />`;
							}
							else if (item.attrbt_type === 'NUM') {
								inputHtml = `<input type="text" class="number" id="${item.attrbt_cd}" name="${item.attrbt_cd}" />`;
							}
							else if (item.attrbt_type === 'SEL') {
								inputHtml = `<div><span class="selectbox" style="z-index: 1;"><select id="${item.attrbt_cd}" name="${item.attrbt_cd}"><option value="" disabled>{{ message("item.com.005") }}</option>`;
								options.forEach(opt => {
									inputHtml += `<option value="${opt.value}">${opt.label}</option>`;
								});
								inputHtml += `</select></span></div>`;
							}
							else if (item.attrbt_type === 'TXTA') {
								inputHtml = `<textarea class="maxlength" id="${item.attrbt_cd}" name="${item.attrbt_cd}" maxlength="2000"></textarea>`;
							}
							else if (item.attrbt_type === 'ATCH') {
								const attachments = (rowData.propertyAttachment || []).filter(att => att.attrbtCd === item.attrbt_cd);

								if (attachments.length > 0) {
									attachments.forEach(att => {
										inputHtml = `
											<div class="uploaded-file" data-file-no="${att.fileNo}">
												<span style = "padding-left: 1rem;">${att.fileNm} (${(att.fileSize / 1024).toFixed(1)} KB)</span>
												<input type="hidden" class="remove-uploaded-file" data-file-no="${att.fileNo}"/>
											</div>
										`;
									});
								}

								inputHtml += `<div class=""><input type="file" class="attach_file" id="${item.attrbt_cd}" name="${item.attrbt_cd}" style="pointer-events: auto; width: fit-content" /></div>`; // style="pointer-events: auto;" component.css에 pointer-events: none; 으로 막혀있음.

							}
							else if (item.attrbt_type === 'HTML') {
								const htmlFormString = item.html_form;  // 기본 HTML 구조
								const savedContent = item.attrbt_cntnts;  // 이전 저장된 데이터(JSON 문자열)

                                renderHtml = htmlFormString;

								if (savedContent && savedContent.trim() !== '' && savedContent.trim() !== '[]') {
									try {
										let parsedDataList = savedContent.split("|"); // 배열로 파싱됨
                                        parsedDataList = [Object.assign({}, ...parsedDataList.map(JSON.parse))];
                                        console.log("변환된 데이터: ", parsedDataList);
										let allGroupHtml = "";

										parsedDataList.forEach(parsedData => {
											// 1. html_formString 하나 복사
											const tempContainer = document.createElement('div');
											tempContainer.innerHTML = htmlFormString;
                                            console.log("tempContainer: ", tempContainer);

											// 2. 각 필드에 값 채워넣기
											Object.keys(parsedData).forEach(key => {
												const value = parsedData[key];
												const field = tempContainer.querySelector(`#${key}`) || tempContainer.querySelector(`[name="${key}"]`);

												if (field) {
													const tagName = field.tagName.toUpperCase();
													const type = field.type?.toLowerCase();

													if (tagName === 'INPUT') {
														if (type === 'radio' || type === 'checkbox') {
															if (field.value === value || value === true) {
																field.setAttribute('checked', 'checked');
															} else {
																field.removeAttribute('checked');
															}
														}
														else if (type === 'number') {
															// , 제거 후 설정
															const numberValue = value.replace(/,/g, '');
															field.setAttribute('value', numberValue);
														}
														else if (type === 'text' || type === 'hidden') {
															field.setAttribute('value', value);
														}
														else if (type === 'date') {
															const date = new Date(value);
															const formattedDate = date.toISOString().split('T')[0]; // YYYY-MM-DD 형식으로 변환
															field.setAttribute('value', formattedDate);
														}
														else {
															field.setAttribute('value', value);
														}
													} else if (tagName === 'TEXTAREA') {
														field.textContent = value;
													} else if (tagName === 'SELECT') {
														Array.from(field.options).forEach(opt => {
															if (opt.value === value) {
																opt.setAttribute('selected', 'selected');
															} else {
																opt.removeAttribute('selected');
															}
														});
													}
												}
											});

											// 3. 완성된 하나의 그룹을 문자열로 추가
											allGroupHtml += tempContainer.innerHTML;
										});

										renderHtml = allGroupHtml; // 전체를 에디터에 출력할 HTML로 설정
									} catch (e) {
										console.error('JSON 배열 파싱 실패:', e);
									}
								}


								// 최종 HTML을 textarea에 설정 (ver.1)
								console.log("renderHtml", renderHtml);
								inputHtml = `<textarea class="html_form" id="${item.attrbt_cd}" name="${item.attrbt_cd}" style="width: 100%; height: 200px;"></textarea>`; // html_form 속성에 HTML 문자열이 들어있다고 가정

								// // 양식 추가 버전(ver.3)
								// inputHtml = `<textarea class="html_form" id="${item.attrbt_cd}" name="${item.attrbt_cd}" style="width: 100%; height: 200px;">${finalHtml}</textarea>
								// 			<button type="button" class="add-form-btn" data-editor-id="${item.attrbt_cd}" style="margin-top: 10px;">양식 추가</button>`;

							}
							else {
								inputHtml = `<input type="text" id="${item.attrbt_cd}" name="${item.attrbt_cd}" maxlength="200" />`;
							}

							rowDiv.innerHTML = `
								<div class="col">
									<div class="form_label">${item.attrbt_name}</div>
									<div class="form_data">${inputHtml}</div>
								</div>
							`;

							dynamicFieldsContainer.appendChild(rowDiv);

							attrCdList.push({
								attrbtCd: item.attrbt_cd,
								attrbtNo: item.attrbt_no,
								attrbtType: item.attrbt_type  // 타입도 같이 저장
							});

							// 기존 값 세팅
							if (item.attrbt_type !== 'ATCH') {  // 파일 속성은 제외
								const $fieldContainer = $(`#dynamicFieldsContainer_update #${item.attrbt_cd}`);

								if (item.attrbt_type === 'HTML') {

									page.document.smartEditor(item.attrbt_cd, item.html_form, renderHtml);

								} else {
									// 기본 타입은 그냥 값 넣기
									$fieldContainer.val(gaiaCommon.decodeSafeText(item.attrbt_cntnts));
								}

							}

							if (item.attrbt_type === 'ATCH') {
								const formDataDiv = rowDiv.querySelector('.form_data');
								if (formDataDiv) {
									formDataDiv.style.display = 'block';
								}
							}

							/* 이벤트 리스너 추가*/
							// .number 클래스에 이벤트 리스너 추가
							rowDiv.querySelectorAll(".number").forEach(input => {
								input.addEventListener("input", function () {
									this.value = this.value.replace(/[^0-9,]/g, ""); // 숫자가 아닌 문자는 제거
								});
							});

							// .maxlength 클래스에 이벤트 리스너 추가

							// .attach_file 클래스에 change 이벤트 리스너 추가
							rowDiv.querySelectorAll(".attach_file").forEach(fileInput => {
								fileInput.addEventListener("change", function () {
									// 새 파일이 선택되었을 때 실행
									const formDataDiv = this.closest('.form_data'); // 현재 파일 input이 있는 form_data
									const uploadedFiles = formDataDiv.querySelectorAll('.uploaded-file');

									uploadedFiles.forEach(uploadedFile => {
										const fileNo = uploadedFile.getAttribute('data-file-no');
										if (fileNo && !page.document.removedSubFiles.includes(fileNo)) {
											page.document.removedSubFiles.push(fileNo);  // 삭제 파일 기록
										}
										uploadedFile.remove(); // 화면에서 삭제
									});
								});
							});

						});
					}

					page.document.attrCdList = attrCdList;

					//그리드 행의 타입이 item형이거나 메뉴가 '착공계 문서 관리'인 경우 파일 업로드 수정 입력창 띄우기
					if (rowData.doc_type === 'ITEM' || documentType === '04') {
						$("#item_update_files").show();
						page.item.fileUpload.init("update", "updateProperty");
						page.item.fileUpload.currentDocNo = rowData.doc_no;

						const attachments = rowData.attachments || [];

						// 파일 목록 업데이트
						const fileList = $("#updateProperty #update_fileList");
						if (fileList) {
							page.item.fileUpload.clearFileList(); // 기존 파일 목록 지우기
							if (attachments.length > 0) {
								page.item.fileUpload.processFetchedFiles(attachments); // 새 파일 목록 추가
							} else {
								page.item.fileUpload.updateFileSizeSummary(); // 파일 사이즈 요약 업데이트
								page.item.fileUpload.updateFileCount(); // 파일 개수 업데이트
							}
						}
					}
					else {
						$("#item_update_files").hide(); //파일 업로드 수정 입력창 숨김.
					}

					$('#updateProperty').addClass('open');
				});

			},

			// HTML 속성 입력을 위한 smarteditor 초기화
			smartEditor(attrbtCd, htmlForm, htmlContent) {
				nhn.husky.EZCreator.createInIFrame({
					oAppRef: editor_object,
					elPlaceHolder: attrbtCd,	// HTML 속성의 ID
					sSkinURI: "/assets/static/smarteditor2/SmartEditor2Skin.html",
					fCreator: "createSEditor2",
					htParams: {
						bUseToolbar : false, // 툴바 사용 여부
						bUseModeChanger : true, // 모드 탭(Editor | HTML | TEXT) 사용 여부 (true:사용/ false:사용하지 않음)
						fOnBeforeUnload: function() { return null; },
					},
					fOnAppLoad : function(){
						let editorFormIndexMap = {}; // 각 editor마다 인덱스를 저장
						const editor = editor_object.getById[attrbtCd];

						if (htmlContent && htmlContent.length > 0) {
							editor.exec("PASTE_HTML", [htmlContent]);
						}

						// 버튼 이벤트 연결 - 양식 추가 버튼 (ver.3)
						const btn = document.querySelector(`.add-form-btn[data-editor-id="${attrbtCd}"]`);
						if (btn) {
							btn.addEventListener("click", function() {
								const parser = new DOMParser();
								const doc = parser.parseFromString(htmlForm, 'text/html');

								// 인덱스 증가: 에디터별 인덱스 관리
								if (!editorFormIndexMap[attrbtCd]) {
									editorFormIndexMap[attrbtCd] = 1;
								} else {
									editorFormIndexMap[attrbtCd]++;
								}
								const index = editorFormIndexMap[attrbtCd];

								// name과 id 속성에 인덱스 추가
								doc.querySelectorAll('input, select, textarea').forEach(el => {
									if (el.name) el.name += `-${index}`;
									if (el.id) el.id += `-${index}`;
									// el.setAttribute('data-group-id', index); // data-group-id 속성 추가
								});

								const updatedHtml = doc.body.innerHTML;
								editor.exec("PASTE_HTML", [updatedHtml]);
							});
						}
						editor.exec("DISABLE_WYSIWYG");
						editor.exec("DISABLE_ALL_UI");
					}
				});
			},

			//문서 속성 정보 수정
			updateProperty() {
				gaiaCommon.LoadingOverlay('body', true); // 로딩 바 활성화

				let requiredValid = true;
				let fileDataModified = false; // 파일 수정 여부를 확인

				let attrbtData = {
                    cntrctNo: page.selectedPjtData.cntrctNo,
					docNo: page.document.data.selected.doc_no,
					docId: page.document.data.selected.doc_id,
					naviId: page.navigation.data.selected.naviId,
					documentType: page.tree.data.documentType,
					attrbtDataList: []
				}


				// 추가 속성 필드 확인
				page.document.attrCdList.forEach(item => {
					const $input = $(`#dynamicFieldsContainer_update #${item.attrbtCd}`);
					const value = $input.val();
					// const value = $(`#dynamicFieldsContainer_update #${item.attrbtCd}`).val();
					const isFileInput = item.attrbtType === 'ATCH';  // 파일 속성 체크
					let fieldData = {}; // 새로운 JSON 오브젝트를 담을 변수
					let finalValue = ''; // 최종적으로 attrbtCntnts에 넣을 값

					let skipPush = false;  // 추가 여부 결정 변수

					// 파일 타입이 아니면 처리 시작
					if (!isFileInput) {
						if (item.attrbtType !== 'HTML') {
							// HTML 타입이 아니면 그냥 일반 value 처리
							finalValue = ($input.val() || '');
						}else{
							// HTML 타입인 경우만 처리
							const firstIframe = document.getElementById(item.attrbtCd)?.nextSibling;
							if (!firstIframe) {
								console.warn(`첫 번째 iframe을 찾을 수 없음 (attrbtCd: ${item.attrbtCd})`);
								return;
							}

							const firstDoc = firstIframe.contentDocument || firstIframe.contentWindow.document;
							const secondIframe = firstDoc.querySelector('iframe.se2_input_wysiwyg');
							if (!secondIframe) {
								console.warn(`두 번째 iframe을 찾을 수 없음 (attrbtCd: ${item.attrbtCd})`);
								return;
							}

							const secondDoc = secondIframe.contentDocument || secondIframe.contentWindow.document;
							const editorBody = secondDoc.querySelector('body');
							if (!editorBody) {
								console.warn(`에디터 본문을 찾을 수 없음 (attrbtCd: ${item.attrbtCd})`);
								return;
							}

                            const formBlocks = editorBody.querySelectorAll('.form-group-block');
                            let objectStrings = []; // JSON 문자열 리스트

                            formBlocks.forEach((block, groupIdx) => {
                                const inputs = block.querySelectorAll('input, select, textarea');

                                inputs.forEach(input => {
                                    const tagName = input.tagName.toUpperCase();
                                    const type = input.type?.toLowerCase();
                                    let name = input.name;
                                    let value = "";

                                    if (!name) return;

                                    if (tagName === 'INPUT') {
                                        if (type === 'radio') {
                                            if (input.checked) value = input.value;
                                            else return;
                                        } else if (type === 'checkbox') {
                                            if (!input.checked) return;
                                            value = input.value;
                                        } else {
                                            value = input.value;
                                        }
                                    } else if (tagName === 'TEXTAREA' || tagName === 'SELECT') {
                                        value = input.value;
                                    }

                                    if (value && value !== "") {
                                        const obj = {};
                                        obj[name] = value;
                                        objectStrings.push(JSON.stringify(obj));
                                    }
                                });
                            });

                            // '|'로 연결된 문자열로 만들기
                            finalValue = objectStrings.join('|');
                            console.log("최종 HTML 속성 문자열:", finalValue);

						}

					}

					console.log("입력 데이터:", finalValue);

					if (isFileInput) {
						const formDataDiv = $input.closest('.form_data');
						const uploadedFile = formDataDiv.find('.uploaded-file');

						if (uploadedFile.length > 0) {
							// 기존 파일이 있으면 굳이 서버에 attrbtCntnts 보낼 필요 없다
							skipPush = true;  // 추가하지 않음
						}
					}

					// push는 skipPush가 false일 때만 한다
					if (!skipPush) {
						attrbtData.attrbtDataList.push({
							attrbtCntnts: finalValue,
							attrbtCd: item.attrbtCd,
							attrbtNo: item.attrbtNo
						});
					}

				});

				if (!requiredValid) return;

				console.log("저장될 속성 정보 값: ", attrbtData);

				const formData = new FormData();
				formData.append("attrbtData", new Blob([JSON.stringify(attrbtData)], { type: "application/json" }));

				//속성 수정할 문서가 아이템형(ITEM)인 경우 혹은 첨부파일 수정 입력창이 있는 경우
				if (page.document.data.selected.doc_type === "ITEM" || documentType === '04') {
					// 제거된 파일 정보 추가
					if (page.item.fileUpload.currentDocNo && page.item.fileUpload.removedFiles[page.item.fileUpload.currentDocNo]) {
						page.item.fileUpload.removedFiles[page.item.fileUpload.currentDocNo].forEach(file => {
							formData.append('removedFiles[]', file.fileNo);
							fileDataModified = true; // 파일 데이터가 수정됨
						});
					}

					// 파일 첨부 창에서 자동으로 추가된 files 필드 제거
					formData.delete('files');
					// 기존 파일 목록인 existingFiles만 수동으로 formData에 추가
					page.item.fileUpload.existingFiles.forEach(file => {
						formData.append('files', file);
						fileDataModified = true; // 파일 데이터가 수정됨
					});
				}

				// 속성 타입(ATCH)로 추가된 파일들 처리
				$("#dynamicFieldsContainer_update .attach_file").each(function () {
					const files = this.files;
					const attrbtCd = this.getAttribute('id'); // id가 attrbtCd와 동일하므로 사용

					if (files && files.length > 0) {
						for (let i = 0; i < files.length; i++) {
							formData.append('subFiles', files[i]); // 기존 파일
							formData.append('subFileAttrbtCd', attrbtCd); // 파일에 해당하는 속성코드 추가
						}
					}
				});

				// 제거된 속성 첨부파일 추가
				if (page.document.removedSubFiles.length > 0) {
					page.document.removedSubFiles.forEach(fileNo => {
						formData.append('removedSubFiles[]', fileNo);
					});
				}

				// 서버에 데이터 전송
                gaiaCommon.postForm("/api/document/property-data/update",formData, function (response) {
                    gaiaCommon.LoadingOverlay('body', false); // 로딩 바 비활성화
                    if (response.ok) {
                        gaiaCommon.customAlert("{{ message('msg.doc.085') }}"); // 속성 데이터가 수정되었습니다.

                    } else {
                        gaiaCommon.customAlert(response.message);
                    }

                    // 파일이 수정된 경우에만 모든 파일 제거 함수 실행
                    if (fileDataModified) {
                        page.item.fileUpload.removeAllFiles();
                        page.item.fileUpload.removedFiles = {}; // 제거된 파일 목록 초기화
                    }

                    // 제거된 파일 번호 목록 초기화
                    page.document.removedSubFiles = [];

                    //모달창 닫기
                    page.popup.closePopup("updateProperty");
                    page.refresh(false, true);
                },function (error) {
                    gaiaCommon.LoadingOverlay('body', false); // 로딩 바 비활성화
                    // 로그인 세션 만료의 경우,
                    if (error.status === 403) {
                        gaiaCommon.customAlert("{{ message('msg.093') }}", function () {
                            if (isProd()) window.close();
                            else location.href="/";
                        });
                    } else {
                        gaiaCommon.customAlert(error.responseJSON?.message);
                    }
                });
			},

