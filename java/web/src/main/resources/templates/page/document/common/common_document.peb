                            copyDoc.forEach(doc => {				// 브레드크럼 정보 업데이트
								page.document.data.breadCrumbList.push(doc);
								lastDoc = doc;
							});
							page.document.setDocBreadcrumb();

							//복사한 문서 위치의 서버데이터 요청
