            makeCurrentDocPath() {
                const list = page?.document?.data?.breadCrumbList ?? [];

                // docNm(없으면 naviNm) → trim → 빈값 제거 → 연속 중복 제거 → ' > '로 조인
                const parts = list
                        .map(it => (it?.docNm ?? it?.naviNm ?? "").toString().trim())
                        .filter(Boolean)
                        .filter((name, i, arr) => i === 0 || name !== arr[i - 1]);

                const path = parts.join(" > ");

                return path; // 필요 시 호출부에서 사용
            },

            //URL 변환 헬퍼 (http/https만 허용, 스킴 없으면 https:// 붙임)
            toAbsoluteUrl(raw, baseOrigin = window.location.origin) {
                if (!raw) return null;
                try {
                    const u = new URL(String(raw).trim(), baseOrigin); // 상대경로 자동 결합
                    if (!/^https?:$/i.test(u.protocol)) return null;   // http/https만 허용
                    return u.href;
                } catch (e) {
                    return null;
                }
            },

            // 결재 문서 팝업 호출 (중앙 정렬, 리사이즈/스크롤 가능)
            openApprovalPopup(url, {width = 1300, height = 800, name = 'approvalPopup'} = {}) {
                const dualLeft = window.screenLeft ?? window.screenX ?? 0;
                const dualTop = window.screenTop ?? window.screenY ?? 0;
                const left = Math.max(0, (window.outerWidth - width) / 2 + dualLeft);
                const top = Math.max(0, (window.outerHeight - height) / 2 + dualTop);

                const features = [
                    `width=${width}`, `height=${height}`,
                    `left=${left}`, `top=${top}`,
                    'resizable=yes', 'scrollbars=yes',
                    'toolbar=no', 'menubar=no', 'location=no', 'status=no'
                ].join(',');

                // popup 차단 방지 & opener 차단
                const win = window.open('about:blank', name, features);
                if (win) {
                    try {
                        win.opener = null;
                    } catch (_) {
                    }
                    win.location.href = url;
                    win.focus();
                } else {
                    // 브라우저가 팝업 차단한 경우 탭으로 대체
                    window.open(url, '_blank', 'noopener,noreferrer');
                }
            }
        }
