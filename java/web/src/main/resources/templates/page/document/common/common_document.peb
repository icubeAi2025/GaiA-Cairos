<style>
    .folder-cell {
        display: flex;
        align-items: center;
        white-space: nowrap;
        overflow: hidden;
        text-overflow: ellipsis;
    }

    .folder-cell .ic {
        color: var(--color-point); /* 폴더 아이콘 색상 - 필요시 조절 */
        font-size: 16px;
        margin-right: 3px;
    }

    .file-cell .ic {
        font-size: 16px;
        margin-right: 3px;
    }
</style>
<script>
    const APPROVAL_BASE = window.location.origin; // 다른 도메인인 경우, 변경 필요. ex) APPROVAL_BASE = "https://approval.com";
	const addRegex = /^(?!\s)[^\/:*?"<>.|]+$/;
	let navigationPopup;
	let propertyPopup;
	let editor_object  = []; // 스마트에디터 객체 초기화
    let availableFileExt;

	document.onkeydown = fkey;
	document.onkeypress = fkey;
	document.onkeyup = fkey;

	// f5 새로고침 누를떄 열려있는 팝업 창 닫기
	function fkey(e) {
		if (window.event.keyCode == 116) {
			if (navigationPopup) navigationPopup.close();
			if (propertyPopup) propertyPopup.close();
		}

	};

	// 문서관리 페이지에서 f2 키 동작 비활성화
	document.addEventListener("keydown", function (e) {
		if (e.key === "F2" || e.keyCode === 113) {
				e.stopPropagation()
				e.preventDefault();
		}
	}, true);

	var page = {
		setAuthMode: null, //권한 설정 모드(navi / doc)
		upDocId: "",
		rootNaviId: "",
		selectedPjtData: {},
		//페이지 상의 state들 담을 객체(선택된 트리의 노드 아이디, 선택된 문서의 docId 리스트, 현재 경로 객체, 등등...)
		data:{},

		//초기화 함수 화면 시작 함수
		init(documentType, menuId) {

			document.querySelector("#btnSendApproval").addEventListener('click', (e) => {
				const grid = page.document.grid;

				if (!grid || typeof grid.getCheckedRows !== 'function') {
					gaiaCommon.customAlert('문서 목록 정보를 불러오지 못했습니다.');
					return;
				}
				const selectedRows = grid.getCheckedRows();

				const folders = [];
				const files = [];
				const itemType = [];
                const completeApprovalDoc = [];
				const cntrctNo = page.tree.data.selectCntrctNo;

				for (const item of selectedRows) {
					if (item.doc_type === 'FOLDR') {
						folders.push(item);
					} else if (item.doc_type === 'ITEM') {
						itemType.push(item);
					} else if (item.ap_link){
                        completeApprovalDoc.push(item);
                    }
                    else {
						files.push(item);
					}
				}

				if (folders.length > 0) {
					gaiaCommon.customAlert('폴더는 선택 할 수 없습니다.');
					return;
				}

				if (itemType.length > 0) {
					gaiaCommon.customAlert('아이템형은 선택 할 수 없습니다.');
					return;
				}

                if (completeApprovalDoc.length > 0) {
                    gaiaCommon.customAlert('이미 결재된 문서는 선택할 수 없습니다.');
                    return;
                }

				if (files.length === 0) {
					gaiaCommon.customAlert('문서를 선택해주세요.');
					return;
				}

				this.data.selectedRows = selectedRows;

				// gaiaCommon.checkAuth("DOC_SEND_03", () => {
					$('#approvalPopupWrap').load(`/document/approval-send-popup?pjtNo=${pjtData.pjtNo}&cntrctNo=${pjtData.cntrctNo}`, function () {
						$('#approvalSendModal').addClass('open');
					});
				// });
			})

			// 휴지통 페이지 생성.
			page.setPageHeader();

			let pjtData = commonJs.getSessionStorage("pageCommonData");
			// console.log("pjtData", pjtData);

			// 프로젝트 정보가 없을 경우,
			if (!pjtData || !pjtData.cntrctNo || !pjtData.pjtNo) {
				gaiaCommon.customAlert("프로젝트 정보가 존재하지 않습니다.");
				return;
			}

			this.selectedPjtData = pjtData;
			this.tree.data.selectCntrctNo = pjtData.cntrctNo;

			// URL에서 파라미터 가져오기
			const urlParams = this.getUrlParameters();

			const param = this.buildParamObject(urlParams, pjtData, documentType, menuId);

			// 데이터 설정
			this.setPageData(param, documentType, menuId);

			if (urlParams.pjtNo && urlParams.cntrctNo) {
				this.checkedLoginInfo(urlParams.pjtNo, urlParams.cntrctNo);
			}

			if (!this.tree || typeof this.tree.init !== 'function') {
				console.warn("트리 객체가 초기화되지 않았습니다.");
				return;
			}

			this.tree.init(urlParams.naviId, documentType);

			// 팝업 이벤트 초기화
			this.popup.clickEventListener();

			//파일 업로드 drag & drop 이벤트 초기화
			this.document.file.setUploadEventListener();

			//TEST
			fileAttachWindow.init("test-component","/api/document/file/create");
			gaia.loaded = true;
		},

		// URL 파라미터 값 가져오기
		getUrlParameters() {
			const urlParams = new URLSearchParams(window.location.search);
			return {
				naviId: urlParams.get('navi_id'),
				docId: urlParams.get('docId'),
				upDocId: urlParams.get('up_doc_id'),
				docNm: urlParams.get('docNm'),
				cntrctNo: urlParams.get('cntrctNo'),
				cntrctNm: urlParams.get('cntrctNm'),
				pjtNo: urlParams.get('pjtNo'),
				pjtNm: urlParams.get('pjtNm'),

			};
		},

		// 파라미터 객체 생성
		buildParamObject(urlParams, pjtData, documentType, menuId) {
			return {
				docId: urlParams.docId || null,
				upDocId: urlParams.upDocId || null,
				docNm: urlParams.docNm || null,
				naviId: urlParams.naviId || null,
				documentType: documentType,
				menuId: menuId,
				cntrctNo: urlParams.cntrctNo || pjtData?.cntrctNo || null,
				cntrctNm: urlParams.cntrctNm || pjtData?.cntrctNm || null,
				pjtNo: urlParams.pjtNo || pjtData?.pjtNo || null,
				pjtNm: urlParams.pjtNm || pjtData?.pjtNm || null
			};
		},

		// 페이지 데이터 설정
		setPageData(param, documentType, menuId) {
			page.upDocId = param.upDocId;					// 상위 문서 id 정보 설정
			page.document.searchData.docNm = param.docNm;	// 문서명 검색 데이터 설정

			page.tree.data.documentType = documentType;		// 트리 정보데이터 설정
			page.tree.data.menuId = menuId;
			page.tree.data.treeParam = param;
            page.tree.data.selectCntrctNo = param.cntrctNo;

            page.selectedPjtData.pjtNo = param.pjtNo;
            page.selectedPjtData.pjt_nm = param.pjtNm;
            page.selectedPjtData.cntrctNo = param.cntrctNo;
            page.selectedPjtData.cntrct_nm = param.cntrctNm;
		},

		// url 복사로 들어온 경우, 유효성 체크
		checkedLoginInfo(pjtNo, cntrctNo) {
			const serviceNm = $("body").attr('class'); 		// Gaia or Cairos

			// ADMIN인 경우, 반환
			if (gaiaCommon.me.info.admin) {
				if (isGAIA() && pjtNo !== cntrctNo) {
					gaiaCommon.customAlert('{{ message("msg.doc.002") }}', function () { // 해당 서비스에서 이동할 수 없는 URL입니다.
						location.replace("/");
						return;
					});
				}
				return;

			}
		},

		// 헤더에 휴지통 아이콘, 통합 검색 창 생성 함수
		setPageHeader() {
            // 문서 관리 구분 값
            const documentMgmtType = documentType;

			// 기존의 page_header 요소 선택
			const pageHeader = document.querySelector('#pageNav');
            pageHeader.style.alignItems = 'baseline';
			const breadCrumbNavi = document.querySelector('.breadcrumb_nav');

			// 기존의 #menuName 요소 삭제
			const existingMenuName = document.querySelector('#menuName');
			const MenuName = existingMenuName.textContent;
			if (existingMenuName) {
				existingMenuName.remove();
			}

			// flex 컨테이너 생성
			const flexContainer = document.createElement('div');
			flexContainer.style.display = 'flex';

			// h2 요소 생성
			const pageTitle = document.createElement('h2');
			pageTitle.className = 'page_tit';
			pageTitle.id = 'menuName';
			// pageTitle.textContent = '{{ message("item.doc.056") }}'; // 통합문서관리
			pageTitle.textContent = MenuName; // 메뉴 위치에 따라 변경
			pageTitle.style.cursor = 'pointer';

			// 휴지통 아이콘 요소 생성
			const deleteIcon = document.createElement('i');
			deleteIcon.className = 'ic ic-delete page_tit';
			deleteIcon.id = 'trash_doc_icon';
			deleteIcon.style.marginLeft = '30px';
			deleteIcon.style.borderBottom = '0px';
			deleteIcon.style.cursor = 'pointer';

            let searchWrap = null;
            if(documentMgmtType === '01'){
                // 통합검색 창 요소 생성
                searchWrap = document.createElement('div');
                searchWrap.className = 'detail_search_wrap';
                searchWrap.style.marginRight = '405px'
                searchWrap.innerHTML = `
                    <div class="searchbox_wrap" style="background-color: white; z-index: 5;">
                        <input type="text" name="doc_all_search_text" id="doc_all_search_text"
                            placeholder="{{ message('msg.004') }}"
                            onkeypress="if(event.keyCode == 13){page.allDetailSearch.detailSearch();}"
                            style="width: 450px;">
                        <!-- 상세검색 아이콘 -->
                        <button type="button" class="btn icon_btn filter" id="all_detail_search_btn" onclick="page.allDetailSearch.detailSearchOpen()">
                            <i class="ic ic-setting-config"></i>
                            <span class="blind">{{ message('item.com.044') }}</span>
                        </button>
                        <button type="button" class="icon_btn search" onclick="page.allDetailSearch.detailSearch()">
                            <i class="ic ic-search"></i>
                            <span class="blind">검색</span>
                        </button>
                        <!-- 상세검색 검색창 ("lay_pop on"일때 보여짐) -->
                        <div class="lay_pop" data-name="search_detail" style="width: 470px;">
                            <button type="button" class="lay_pop_close icon_btn" onclick="page.allDetailSearch.detailSearchClose()" style="position: absolute;">
                                <i class="ic ic-close"></i>
                                <span class="blind">{{ message('item.com.037') }}</span>
                            </button>

                            <div class="form_box ">
                                <div class="row">
                                    <div class="col">
                                        <div class="form_label">폴더분류</div>
                                        <div class="form_data">
                                            <span class="selectbox" id="detail_naviFolderType_box">
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="btn_group _fill s_small jc_e">
                                <button type="button" class="btn" onclick="page.allDetailSearch.detailSearch()">
                                    {{message('btn.014') }}
                                </button>
                                <button type="button" class="btn" onclick="page.allDetailSearch.detailSearchReset()">
                                    {{message('btn.024')}}
                                </button>
                            </div>
                        </div>
                    </div>
                `;

            }

			// flex 컨테이너에 h2와 아이콘 추가
			flexContainer.appendChild(pageTitle);
			flexContainer.appendChild(deleteIcon);

			// pageHeader에 flex 컨테이너 추가
			pageHeader.appendChild(flexContainer);
            if(searchWrap){
			    pageHeader.appendChild(searchWrap);
            }
			pageHeader.appendChild(breadCrumbNavi);


			// 이벤트 리스너 추가
			deleteIcon.addEventListener('click', () => {
				deleteIcon.style.removeProperty('border-bottom');
				pageTitle.style.borderBottom = '0px';
				this.trash.init();
			});

			pageTitle.addEventListener('click', () => {
				pageTitle.style.removeProperty('border-bottom');
				deleteIcon.style.borderBottom = '0px';
				this.trash.close();
			});
		},

		/* 휴지통 페이지 설정 */
		trash: {
			selectedCntrctNo: null,
			grid: null,

			init() {
				$("#trash_doc_page").show();
				$("#mng_doc_page").hide();
				$("#search_doc_page").hide();
				this.selectedCntrctNo = page.tree.data.selectCntrctNo;
				this.gridInit();
			},

			close() {
				$("#trash_doc_page").hide();
				$("#mng_doc_page").show();
                $("#search_doc_page").hide();

				this.trashSearchClear();

				page.refresh(true, true);
			},

			gridInit: function () {
				if (this.grid) { // 이미 그리드가 있으면 제거하고 새로 생성.
					this.grid.destroy();
					this.grid = null;
				}

				if (!this.grid) {
					this.grid = new tui.Grid({
						el: document.getElementById('trash_document_grid'),
						scrollX: true,
						scrollY: true,
						useClientSort: false,
						bodyHeight: window.innerHeight - 265,
						minBodyHeight: 40, // 최소 높이 : row 1개
						width: 'auto',
						contextMenu: null,
						rowHeaders: [
							{
								type: 'checkbox',
							},

						],
						columns: [
							{
