					sourceItemNo: sourceItemNo,
					sourceItemKind: sourceItemKind,
					sourceItemPath: sourceItemPath,
					targetDocIdList: targetDocIdList
				}

				gaiaCommon.post("api/document/copy", reqData, function (result) {
