<!doctype html>
<script>

function initPageSet(nav, event)  {
    $('a').removeClass('active');
    $(event.target).addClass('active');

    const ol = document.getElementById('nav_list');
    const li_items = ol.getElementsByTagName('li');
    if(li_items.length > 2) {
        li_items[2].remove();
    }

    var ul_list = $("#nav_list");
    ul_list.append("<li class='breadcrumb_item'><a style='pointer-events:none;' href='javascript:void(0);'><span>" + $(event.target).text() + "</span></a></li>");
}

function setNav(menuName)  {    
    var ul_list = $("#nav_list");
    ul_list.append("<li class='breadcrumb_item'><a style='pointer-events:none;' href='javascript:void(0);'><span>" + menuName + "</span></a></li>");

    $("li a:contains(" + menuName + ")").addClass("active");
}

</script>
<html lang="ko">
    <head>
    {% include "sub/head" %}
    {% block head %}{% endblock %}
    </head>

    <body class="layout-fixed sidebar-collapse">
        <div class="wrapper add_wrapper">

            <!-- NAVBAR -->
            {% include "sub/gnb_home" %}
            <!-- // NAVBAR -->
        
            <!-- CONTENT  -->
            <div class="content-wrapper-c">
                <div class="container container-pmc">
                    
                    <!-- LEFT NAV -->
                    {% include "sub/doc_approval_nav" %}
                    <!-- // LEFT NAV -->
                
                    <!-- RIGHT MAIN -->
                    <div class="content_wrap">

                        <!-- BREADCRUMB -->
                        <nav class="breadcrumb_nav">
                            <ol class="breadcrumb" id="nav_list">
                                <li class="breadcrumb_item"><a href="/" style="pointer-events:none;">GAIA</a></li>
                                <li class="breadcrumb_item"><a style="pointer-events:none;" href="javascript:void(0);"><span>전자결재</span></a></li>        
                            </ol>
                        </nav>
                        <!-- // BREADCRUMB -->

                        <!-- MAIN CONTENT -->
                        <main class="content content-wrapper-c-storage">
                            <div class="g-group g-row">       
                                <div class="content-wrapper-c  content-wrapper-c-iframe iframe-mode" data-widget="iframe" data-loading-screen="0">
                                
                                    {% block content %}{% endblock %}

                                </div>
                            </div>
                        </main>
                        <!-- // MAIN CONTENT -->

                    </div>
                    <!-- // RIGHT MAIN -->
                </div>
            </div>
            <!-- // CONTENT -->
        
        </div>
        {% include "sub/footer" %}
        {% include "sub/grid" %}
        {% block footer_script %}{% endblock %}
    </body>
</html>