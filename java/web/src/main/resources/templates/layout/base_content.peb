<!DOCTYPE html>
<html lang="ko">

<head>
	{% include "sub/head" %}
	{% block head %}{% endblock %}
</head>

<body style="display:none">
	{% include "sub/head_content" %}
	<div class="container">
		<!-- 좌측 메뉴 영역 (Navbar) Start -->
		{% include "sub/nav" %}
		<!-- 좌측 메뉴 영역 (Navbar) End -->
		<div class="content_wrap">
			<div class="page_header" id="pageNav"></div>

			<div id="content" style="position: relative; padding-right: 120px">
				<!-- 화면 본 컨텐츠 영역 Start -->
				{% block content %}{% endblock %}
				<!-- 화면 본 컨텐츠 영역 End -->

				<aside class="dashboard_sec _side"
					style="position: fixed; right: 5px; top:120px; padding: 1em; pointer-events: none;">
					<dl class="side_box" style="margin: 0; width: 100px; background-color: var(--color-white);">
						<!-- 결재 -->
						<dt>{{ message('item.app.008') }}</dt> <!-- 결재 -->
						<dd>
							<ul class="list_sty audit_list" id="auditList">
							</ul>
						</dd>
					</dl>
					<br>
					<dl class="side_box" id="requestSide" style="margin: 0; width: 100px; background-color: var(--color-white); display: none;">
						<!-- 검측요청 -->
						<dt>검측요청</dt> <!-- 검측요청 -->
						<dd>
							<ul class="list_sty audit_list" id="requestList">
							</ul>
						</dd>
					</dl>
					<!-- Section Side -->
					<!-- 바로가기 버튼 -->
					<div class="q_links dashboard_quick" style="display: none; ">
						<a href="#" class="link_btn _cmis" style="display: none;">{{ message('item.dash.034') }}</a>
						<a href="#" class="link_btn _cctv" style="display: none;">{{ message('item.dash.035') }}</a>
					</div>

					<!-- 날씨 -->
					<div class="weather dashboard_quick" style="width: 100px; display: none;">
						<div class="weather_item">
							<time class="updated_at" datetime=""></time>
							<p class="location"></p>
						</div>
						<div class="weather_item tmp">
							<strong class="today_tmp"></strong>
							<p class="minmax">
								<span class="min"></span>
								<span class="max"></span>
							</p>
							<span class="rainfall"></span>
						</div>
						<div class="weather_item">
							<span class="sunrise">일출 : <time datetime="">-</time></span>
							<span class="sunset">일몰 : <time datetime="">-</time></span>
						</div>
					</div>
					<!-- // 날씨 -->
				</aside>
				<div id="up-btn" style="display: none; position: fixed; right: 5px; bottom:0px; padding: 1em;">
					<button class="icon_btn" style="color: var(--color-default);opacity:60%;">
						<i class="fas fa-arrow-circle-up fa-2x"
							style="background-color: var(--color-white); border-radius: 100%;"></i>
					</button>
				</div>
				{% include "sub/footer" %}
			</div>
		</div>

		{% include "sub/pop_box" %}
	</div>

	{% block footer_script %}
	<script>
	</script>
    {% endblock %}
</body>

</html>