	<xsd:complexType name="공사원가호환규정형식">
		<xsd:sequence>
			<xsd:element name="스키마버전">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
					  <xsd:enumeration value="1.0.4"/>
					  <xsd:enumeration value="1.0.3"/>
					  <xsd:enumeration value="1.0.2"/>
						<xsd:enumeration value="1.0.1"/>
						<xsd:enumeration value="1.0.0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="작성정보" type="작성정보형식"/>
			<xsd:element name="공사정보" type="공사정보형식"/>
			<xsd:element name="공사내역서" type="공사내역서형식"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="공사정보형식">
