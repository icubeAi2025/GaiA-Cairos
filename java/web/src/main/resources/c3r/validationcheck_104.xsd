						<xsd:enumeration value="관급자재공종"/>
						<xsd:enumeration value="일반공종"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="계산제외">
				<xsd:simpleType>
					<xsd:restriction base="xsd:boolean"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="공종번호" type="번호형식"/>
			<xsd:element name="공종명" type="품명형식"/>
			<xsd:element name="재료비" type="xsd:double"/>
			<xsd:element name="노무비" type="xsd:double"/>
			<xsd:element name="경비" type="xsd:double"/>
			<xsd:element name="비고" type="비고형식"/>
			<xsd:element name="기타" type="기타형식" minOccurs="0"/>
			<xsd:element name="세부공종" type="세부공종형식" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="세부공종형식">
		<xsd:sequence>
			<xsd:element name="세부공종코드" type="내역서코드형식"/>
			<xsd:element name="자원구분">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="자재"/>
						<xsd:enumeration value="노무"/>
						<xsd:enumeration value="경비항목"/>
						<xsd:enumeration value="일위대가"/>
						<xsd:enumeration value="중기단가산출"/>
						<xsd:enumeration value="기계경비"/>
						<xsd:enumeration value="실적공사비"/>
						<xsd:enumeration value="시장시공가격"/>
						<xsd:enumeration value="손료"/>
						<xsd:enumeration value="텍스트"/>
						<xsd:enumeration value="표준시장단가"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="계산제외">
				<xsd:simpleType>
					<xsd:restriction base="xsd:boolean"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="세부공종번호" type="번호형식"/>
			<xsd:element name="세부공종명" type="품명형식"/>
			<xsd:element name="규격" type="규격형식"/>
			<xsd:element name="단위" type="단위형식"/>
			<xsd:element name="수량" type="xsd:double"/>
			<xsd:element name="재료비단가" type="xsd:double"/>
			<xsd:element name="노무비단가" type="xsd:double"/>
			<xsd:element name="경비단가" type="xsd:double"/>
			<xsd:element name="재료비" type="xsd:double"/>
			<xsd:element name="노무비" type="xsd:double"/>
			<xsd:element name="경비" type="xsd:double"/>
			<xsd:element name="관급">
				<xsd:simpleType>
					<xsd:restriction base="xsd:boolean"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="자재구분">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="구분없음"/>
						<xsd:enumeration value="분리발주"/>
						<xsd:enumeration value="주요자재"/>
						<xsd:enumeration value="일반자재"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="세부공종구분">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="일반내역"/>
						<xsd:enumeration value="무대내역"/>
						<xsd:enumeration value="PS내역"/>
						<xsd:enumeration value="TAB내역"/>
						<!--<xsd:enumeration value="관급자재" />-->
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="손료코드군" type="손료코드군형식" minOccurs="0"/>
			<xsd:element name="손료계산비목구분" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="재료비"/>
						<xsd:enumeration value="노무비"/>
						<xsd:enumeration value="경비"/>
						<xsd:enumeration value="합계"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="손료결과비목구분" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="재료비"/>
						<xsd:enumeration value="노무비"/>
						<xsd:enumeration value="경비"/>
						<xsd:enumeration value="합계"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="손료적용요율" type="xsd:double" minOccurs="0"/>
			<xsd:element name="손료처리자리수" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="10"/>
						<xsd:enumeration value="100"/>
						<xsd:enumeration value="1000"/>
						<xsd:enumeration value="10000"/>
						<xsd:enumeration value="100000"/>
						<xsd:enumeration value="1000000"/>
						<xsd:enumeration value="10000000"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="손료처리방법" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="C"/>
						<xsd:enumeration value="R"/>
						<xsd:enumeration value="U"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="노임산출근거" type="노임산출근거형식" minOccurs="0"/>
			<xsd:element name="비고" type="비고형식"/>
			<xsd:element name="기타" type="기타형식" minOccurs="0"/>
			<xsd:element name="WBS코드1" type="비고형식" minOccurs="0"/>
			<xsd:element name="WBS코드2" type="비고형식" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="단일공사정보형식">
		<xsd:sequence>
			<xsd:element name="공사구분">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="토목공사"/>
						<xsd:enumeration value="건축공사"/>
						<xsd:enumeration value="설비공사"/>
						<xsd:enumeration value="전기공사"/>
						<xsd:enumeration value="통신공사"/>
						<xsd:enumeration value="조경공사"/>
						<xsd:enumeration value="소방공사"/>
						<xsd:enumeration value="문화재수리공사"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="공사설명">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="100"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="전역변수" type="전역변수형식" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="전역변수형식">
		<xsd:sequence>
			<xsd:element name="변수명">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="10"/>
						<xsd:pattern value="[a-zA-Z_]+[0-9a-zA-Z_]*"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="실수값" type="xsd:double"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="개별노임산출형식">
		<xsd:sequence>
			<xsd:element name="노무코드">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="16"/>
						<xsd:minLength value="1"/>
						<xsd:pattern value="[A-Z0-9]+"/>
						<!--<xsd:pattern value="[L]{1}[A-Z0-9]+" /> 표준노무코드 적용시-->
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="공량" type="xsd:double"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="직접공사비정보">
		<xsd:sequence>
			<xsd:element name="직접공사비_재료비" type="xsd:long"/>
			<xsd:element name="직접공사비_노무비" type="xsd:long"/>
			<xsd:element name="직접공사비_경비" type="xsd:long"/>
			<xsd:element name="미확정설계공종_금액" type="xsd:long"/>
			<xsd:element name="제요율적용제외공종_금액" type="xsd:long"/>
			<xsd:element name="관급자재공종_금액" type="xsd:long"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="제경비항목정보">
		<xsd:sequence>
			<xsd:element name="세부공종코드" type="내역코드형식"/>
			<xsd:element name="집계항목여부" type="xsd:boolean"/>
			<xsd:element name="비목명" type="품명형식"/>
			<xsd:element name="적용산식">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="200"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="적용요율" type="xsd:double"/>
			<xsd:element name="추가금액" type="xsd:double"/>
			<xsd:element name="산출금액" type="xsd:double"/>
			<xsd:element name="적용산식설명">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="200"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="공사제경비형식">
		<xsd:sequence>
			<xsd:element name="제경비항목" type="제경비항목정보" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="기계경비상세">
		<xsd:sequence>
			<xsd:element name="자원구분">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="자재"/>
						<xsd:enumeration value="노무"/>
						<xsd:enumeration value="경비항목"/>
						<xsd:enumeration value="기계경비"/>
						<xsd:enumeration value="손료"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="자원코드" type="하위코드형식"/>
			<xsd:element name="계산제외">
				<xsd:simpleType>
					<xsd:restriction base="xsd:boolean"/>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="품명" type="품명형식"/>
			<xsd:element name="규격" type="규격형식"/>
			<xsd:element name="단위" type="단위형식"/>
			<xsd:element name="수량" type="xsd:double"/>
			<xsd:element name="재료비단가" type="xsd:double"/>
			<xsd:element name="노무비단가" type="xsd:double"/>
			<xsd:element name="경비단가" type="xsd:double"/>
			<xsd:element name="재료비" type="xsd:double"/>
			<xsd:element name="노무비" type="xsd:double"/>
			<xsd:element name="경비" type="xsd:double"/>
			<xsd:element name="손료코드군" type="손료코드군형식" minOccurs="0"/>
			<xsd:element name="손료계산비목구분" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="재료비"/>
						<xsd:enumeration value="노무비"/>
						<xsd:enumeration value="경비"/>
						<xsd:enumeration value="합계"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="손료결과비목구분" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="재료비"/>
						<xsd:enumeration value="노무비"/>
						<xsd:enumeration value="경비"/>
						<xsd:enumeration value="합계"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="손료적용요율" type="xsd:double" minOccurs="0"/>
			<xsd:element name="상각비할증" type="xsd:double" minOccurs="0"/>
			<xsd:element name="정비비할증" type="xsd:double" minOccurs="0"/>
			<xsd:element name="관리비할증" type="xsd:double" minOccurs="0"/>
			<xsd:element name="비고" type="비고형식"/>
			<xsd:element name="기타" type="기타형식" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="사용자정의_시간당노임산출정보">
		<xsd:sequence>
			<xsd:element name="사용자정의코드" type="시간당노임산출코드형식"/>
			<xsd:element name="노임산출" type="노임산출형식"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="번호형식">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="20"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="내역서코드형식">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="17"/>
			<xsd:pattern value="([0-9A-Z])*"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="코드형식">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="17"/>
			<xsd:pattern value="([0-9A-Z])*"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="실적공사비코드형식">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="12"/>
			<xsd:pattern value="[ACEMT]+[0-9A-Z]*"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="세부공종코드형식">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="12"/>
			<xsd:pattern value="[0-9A-Z]*"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="기계경비코드형식">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="17"/>
			<xsd:pattern value="([0-9A-Z])*"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="자재코드형식">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="16"/>
			<xsd:pattern value="([0-9A-Z])*"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="노무코드형식">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="16"/>
			<xsd:pattern value="([0-9A-Z])*"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="경비항목코드형식">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="16"/>
			<xsd:pattern value="([0-9A-Z])*"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="하위코드형식">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="17"/>
			<xsd:pattern value="([0-9A-Z])*"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="시간당노임산출코드형식">
		<xsd:restriction base="xsd:ID">
			<xsd:length value="2"/>
			<xsd:pattern value="([H][A-Z])"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="품명형식">
		<xsd:restriction base="xsd:normalizedString">
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="규격형식">
		<xsd:restriction base="xsd:normalizedString">
			<xsd:maxLength value="100"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="손료코드군형식">
		<xsd:restriction base="xsd:normalizedString">
			<xsd:maxLength value="500"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="단위형식">
		<xsd:restriction base="xsd:normalizedString">
			<xsd:maxLength value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="비고형식">
		<xsd:restriction base="xsd:normalizedString">
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="기타형식">
		<xsd:restriction base="xsd:normalizedString">
			<xsd:maxLength value="100"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="손료산출식형식">
		<xsd:restriction base="xsd:normalizedString">
			<xsd:maxLength value="200"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="내역공종코드형식">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="2"/>
			<xsd:maxLength value="20"/>
			<xsd:pattern value="([0-9][0-9])+"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="노임산출형식">
		<xsd:sequence>
			<xsd:element name="산출식" type="손료산출식형식"/>
			<xsd:element name="계산값" type="xsd:double"/>
			<xsd:element name="적용구분">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="산출식"/>
						<xsd:enumeration value="계산값"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="기계경비산출형식">
		<xsd:sequence>
			<xsd:element name="환율" type="환율형식" minOccurs="0"/>
			<xsd:element name="시간당손료산출" type="시간당손료산출형식" minOccurs="0"/>
			<xsd:element name="시간당노임산출" type="시간당노임산출형식" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="환율형식">
		<xsd:sequence>
			<xsd:element name="미국_1USD" type="xsd:double"/>
			<xsd:element name="유럽연합_1EUR" type="xsd:double"/>
			<xsd:element name="일본_100엔" type="xsd:double"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="시간당손료산출형식">
		<xsd:sequence>
			<xsd:element name="E0-주간" type="손료산출식형식" minOccurs="0"/>
