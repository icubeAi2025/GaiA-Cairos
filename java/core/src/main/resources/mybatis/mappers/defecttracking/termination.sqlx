				<if test='searchInput.myRplyYn != null and searchInput.myRplyYn != "" and searchInput.myRplyYn.equals("Y")'>
					AND (D.QA_RGSTR_ID = #{usrId} OR D.SPVS_RGSTR_ID = #{usrId} OR R.RPLY_RGSTR_ID = #{usrId} OR D.RGSTR_ID = #{usrId})
				</if>
