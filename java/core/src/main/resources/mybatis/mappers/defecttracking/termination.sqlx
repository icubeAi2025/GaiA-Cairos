						</when>
						<when test="searchInput.spvsStatus == 'ing'">
							AND (D.QA_CD IS NULL OR D.QA_CD = '') AND DDS2.END_DATE &gt;= CURRENT_DATE
						</when>
						<when test="searchInput.spvsStatus == 'end'">
							AND (D.QA_CD IS NULL OR D.QA_CD = '') AND DDS2.BGN_DATE &lt;= CURRENT_DATE AND DDS2.END_DATE &lt;= CURRENT_DATE
						</when>
						<otherwise>
							AND 1=1
						</otherwise>
					</choose>
				</if>
				<if test="searchInput.spvsCd != null and searchInput.spvsCd != ''">
					AND D.SPVS_CD = #{searchInput.spvsCd}
				</if>
                <if test="searchInput.edStatus != null and searchInput.edStatus != ''">
					<choose>
						<when test="searchInput.edStatus == 'ok'">
							AND D.ED_CD IS NOT NULL AND D.ED_CD &lt;&gt; ''
						</when>
						<when test="searchInput.edStatus == 'ing'">
							AND (D.ED_CD IS NULL OR D.ED_CD = '') AND DDS2.END_DATE &gt;= CURRENT_DATE
						</when>
						<when test="searchInput.edStatus == 'end'">
							AND (D.ED_CD IS NULL OR D.ED_CD = '') AND DDS2.BGN_DATE &lt;= CURRENT_DATE AND DDS2.END_DATE &lt;= CURRENT_DATE
						</when>
						<otherwise>
