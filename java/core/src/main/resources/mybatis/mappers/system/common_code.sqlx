<mapper namespace="kr.co.ideait.platform.gaiacairos.mybatis.mappers.system.commoncode">
    <select id="selectListByCmnGrpCd" resultType="smComCodeGroup">
        /* [kr.co.ideait.platform.gaiacairos.mybatis.mappers.system.commoncode.selectListByCmnGrpCd][jhkim]  */
        SELECT
            A.CMN_GRP_NO,
            A.CMN_GRP_CD,
            A.CMN_CD,
            A.CMN_CD_NM_ENG,
            A.CMN_CD_NM_KRN,
            A.CMN_CD_DSPLY_ORDR,
            A.CMN_CD_DSCRPT,
            A.UP_CMN_GRP_NO,
            A.UP_CMN_GRP_CD,
            A.PUBLIC_YN,
            A.CMN_LEVEL,
            A.USE_YN,
            TO_CHAR(A.CHG_DT, 'FMYYYY-MM-DD') AS CHG_DT,
            (SELECT B.USR_NM
            FROM SM_USER_INFO B
            WHERE B.USR_ID = A.CHG_ID
            LIMIT 1) AS CHG_ID
        FROM
            SM_COM_CODE_GROUP A
        <where>
            <if test="list != null and list.size() > 0">
                AND A.CMN_GRP_CD IN
                <foreach item="item" collection="list" open="(" separator="," close=")">
                    #{item}
                </foreach>
            </if>
            AND A.DLT_YN = 'N'
        </where>
        ORDER BY CMN_CD_DSPLY_ORDR;
    </select>

