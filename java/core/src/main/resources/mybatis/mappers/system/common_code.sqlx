    <update id="updateSmComCodeGroup">
        /* [kr.co.ideait.platform.gaiacairos.mybatis.mappers.system.commoncode.updateSmComCodeGroup][jhkim]  */
        /* MOD - [kr.co.ideait.platform.gaiacairos.mybatis.mappers.system.commoncode.updateSmComCodeGroup][dsjung]  */
        UPDATE SM_COM_CODE_GROUP
        <set>
            <if test=' cmnCd != null and !"".equals(cmnCd) '>
                , cmn_cd = #{cmnCd}
            </if>
            <if test=' cmnCdNmEng != null'>
                , cmn_cd_nm_eng = #{cmnCdNmEng}
            </if>
            <if test=' cmnCdNmKrn != null'>
                , cmn_cd_nm_krn = #{cmnCdNmKrn}
            </if>
            <if test=' cmnCdDsplyOrdr != null and !"".equals(cmnCdDsplyOrdr) '>
                , cmn_cd_dsply_ordr = #{cmnCdDsplyOrdr}
            </if>
            <if test=' cmnCdDscrpt != null '>
                , cmn_cd_dscrpt = #{cmnCdDscrpt}
            </if>
            <if test=' upCmnGrpNo != null and !"".equals(upCmnGrpNo) '>
                , up_cmn_grp_no = #{upCmnGrpNo}
            </if>
            <if test=' upCmnGrpCd != null and !"".equals(upCmnGrpCd) '>
                , up_cmn_grp_cd = #{upCmnGrpCd}
            </if>
            <if test=' publicYn != null and !"".equals(publicYn) '>
                , public_yn = #{publicYn}
            </if>
            <if test=' cmnLevel != null and !"".equals(cmnLevel) '>
                , cmn_level = #{cmnLevel}
            </if>
            <if test=' useYn != null and !"".equals(useYn) '>
                , use_yn = #{useYn}
            </if>
            <if test=' dltYn != null and !"".equals(dltYn) '>
                ,dlt_yn = #{dltYn}
                <if test=' "Y".equals(dltYn) '>
                    ,dlt_id = #{dltId}
                    ,dlt_dt = NOW()
                </if>
            </if>
            , chg_id = #{chgId}
            , chg_dt = NOW()
        </set>
        <where>
            AND CMN_GRP_CD = #{cmnGrpCd}
        </where>
    </update>

